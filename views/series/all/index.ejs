<% include ../../partials/header %>

  <form action="/series/all/delete?_method=DELETE" method="POST">
    <section>
    <ul class="content-list">
      <div class="actions">
        <a class="btn btn-return" href="/series">Return</a>
        <button class="btn btn-confirm">Clear</button>
      </div>
        <% allSeries.forEach(function(series) { %>
          <li>
            <p><%= series.bot %></p>
            <p><%= series.name %></p>
            <p><%= series.current %></p>
            <input name="delete" type="checkbox" value="<%= series._id %>" />
          </li>
        <% }); %>
      </ul>
    </section>
  </form>

<% include ../../partials/footer %>
